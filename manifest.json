{
  "name": "transit_bmks",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["script.js"]
  }],
  "browser_action": {
    "default_icon": {
      "19": "images/shooting_star19.png"
    }
  },
  "icons": {
    "16": "images/shooting_star16.png",
    "32": "images/shooting_star32.png",
    "48": "images/shooting_star48.png",
    "128": "images/shooting_star128.png"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "background",
    "http://*/*",
    "https://*/*"
  ]
}
